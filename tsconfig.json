{
    "compilerOptions": {
      "target": "es5",
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "baseUrl": "src"
    },
    "include": [
      "./src/**/*"
    ]
  }
  
  